{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetBucketLocation",
        "s3:ListBucket"
      ],
     "Resource": [
        "arn:aws:s3:::${aws_s3_bucket.s3_versioned_bucket.bucket}"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObject",
                "s3:GetObjectAcl",
                "s3:DeleteObject"
      ],
      "Resource": [
         "arn:aws:s3:::${aws_s3_bucket.s3_versioned_bucket.bucket}/*"
      ]
    }
  ]
}
